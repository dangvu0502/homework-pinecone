{
  "name": "code-challenge",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "node --env-file=.env --watch src/server.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "migrate:latest": "npx knex migrate:latest --knexfile src/knexfile.ts",
    "migrate:make": "npx knex migrate:make --knexfile src/knexfile.ts",
    "migrate:rollback": "npx knex migrate:rollback --knexfile src/knexfile.ts",
    "migrate:status": "npx knex migrate:status --knexfile src/knexfile.ts",
    "migrate:down": "npx knex migrate:down --knexfile src/knexfile.ts"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^6.1.2",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.87.1",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "knex": "^3.1.0",
    "langchain": "^0.3.33",
    "multer": "^2.0.2",
    "openai": "^5.19.1",
    "pdfjs-dist": "^5.4.149",
    "sqlite3": "^5.1.7",
    "uuid": "^12.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.1",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  }
}
